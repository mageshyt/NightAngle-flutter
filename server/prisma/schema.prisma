generator client {
    provider  = "prisma-client-py"
    interface = "asyncio"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model User {
    id       String @id @default(uuid())
    name     String
    email    String @unique
    password String
    songs    Songs[]
}

model Songs{
    id String @id @default(uuid())
    song_name String
    thumbnail_url String
    song_url String
    artist String
    hex_color String
    user User @relation(fields: [userId], references: [id])
    userId String
}